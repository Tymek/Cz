    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "2.5.0",
    "sass": "^1.43.4",
    "svelte": "3.44.2",
    "svelte-check": "2.2.10",
